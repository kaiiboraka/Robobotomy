void fragment() {
// FloatParameter:14
	float n_out14p0 = Rotation;


	vec2 n_out13p0;
// Expression:13
	n_out13p0 = vec2(0.0, 0.0);
	{
		// vec2 rotate(vec2 n_out13p0, vec2 pivot, float n_out14p0)
		// {
		n_out13p0 = UV;
		vec2 pivot = vec2(0);
		
		mat2 rotation = mat2(vec2(sin(radians(n_out14p0)), -cos(radians(n_out14p0))),
							vec2(cos(radians(n_out14p0)), sin(radians(n_out14p0))));
			
		n_out13p0 -= pivot;
		n_out13p0 = n_out13p0 * rotation;
		n_out13p0 += pivot;
			// return n_out13p0;
		// }
		
		// n_out13p0 = rotate(UV, vec2(0.5), rotation);
	}


// FloatParameter:8
	float n_out8p0 = Tiling;


// VectorCompose:10
	vec2 n_out10p0 = vec2(n_out8p0, n_out8p0);


// UVFunc:7
	vec2 n_in7p2 = vec2(0.00000, 0.00000);
	vec2 n_out7p0 = (n_out13p0 - n_in7p2) * n_out10p0 + n_in7p2;


// Texture2D:2
	vec4 n_out2p0 = texture(tex_frg_2, n_out7p0);


	vec3 n_out17p0;
// ColorFunc:17
	{
		vec3 c = vec3(n_out2p0.xyz);
		vec4 K = vec4(0.0, -1.0 / 3.0, 2.0 / 3.0, -1.0);
		vec4 p = mix(vec4(c.bg, K.wz), vec4(c.gb, K.xy), step(c.b, c.g));
		vec4 q = mix(vec4(p.xyw, c.r), vec4(c.r, p.yzx), step(p.x, c.r));
		float d = q.x - min(q.w, q.y);
		float e = 1.0e-10;
		n_out17p0 = vec3(abs(q.z + (q.w - q.y) / (6.0 * d + e)), d / (q.x + e), q.x);
	}


// FloatParameter:19
	float n_out19p0 = Hue;


// FloatParameter:18
	float n_out18p0 = Saturation;


// FloatParameter:20
	float n_out20p0 = Value;


	vec3 n_out16p0;
	float n_out16p1;
	float n_out16p2;
	float n_out16p3;
// Expression:16
	n_out16p0 = vec3(0.0, 0.0, 0.0);
	n_out16p1 = 0.0;
	n_out16p2 = 0.0;
	n_out16p3 = 0.0;
	{
		// userIn *= 100.0;
		float hue = n_out17p0.x;
		float sat = n_out17p0.y;
		float val = n_out17p0.z;
		
		float hScale;
		float sScale;
		float vScale;
		
		if (n_out19p0 < 0.0) {
			hScale = hue;
		} else if (n_out19p0 > 0.0) {
			hScale = 1.0 - hue;
		}
		
		if (n_out18p0 < 0.0) {
			sScale = sat;
		} else if (n_out18p0 > 0.0) {
			sScale = 1.0 - sat;
		}
		
		if (n_out20p0 < 0.0) {
			vScale = val;
		} else if (n_out20p0 > 0.0) {
			vScale = 1.0 - val;
		}
		
		n_out16p1 = hue + (n_out19p0 * hScale);
		n_out16p2 = sat + (n_out18p0 * sScale);
		n_out16p3 = val + (n_out20p0 * vScale);
		
		// if (n_out16p1 > 1.0) {
			// n_out16p1 = 1.0;
		// } else if (n_out16p1 < 0.0) {
			// n_out16p1 = 0.0;
		// }
		
		n_out16p0 = vec3(n_out16p1, n_out16p2, n_out16p3);
		// vec3 n_out16p0 = vec3(hue, sat, val);
	}


	vec3 n_out15p0;
// ColorFunc:15
	{
		vec3 c = n_out16p0;
		vec4 K = vec4(1.0, 2.0 / 3.0, 1.0 / 3.0, 3.0);
		vec3 p = abs(fract(c.xxx + K.xyz) * 6.0 - K.www);
		n_out15p0 = c.z * mix(K.xxx, clamp(p - K.xxx, 0.0, 1.0), c.y);
	}


// Texture2D:5
	vec4 n_out5p0 = texture(tex_frg_5, n_out7p0);


// Texture2D:3
	vec4 n_out3p0 = texture(tex_frg_3, n_out7p0);


	vec4 n_out11p0;
// Texture2D:11
	n_out11p0 = texture(Edge_Map, UV);


// Output:0
	ALBEDO = n_out15p0;
	METALLIC = n_out5p0.x;
	ROUGHNESS = n_out3p0.x;
	NORMAL_MAP = vec3(n_out11p0.xyz);


}
